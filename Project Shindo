--> Visual

local Tab = Window:MakeTab({
	Name = " ðŸ‘€ Visual ",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Visual Scripts (SÃ³ vocÃª pode ver) "
})

Tab:AddSlider({
	Name = "Infinite Level",
	Min = 10000,
	Max = 10000000000000000,
	Default =10000,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Level",
	Callback = function(Level)
		game.Players.LocalPlayer.statz.lvl.lvl.Value = Level
	end    
})
Tab:AddSlider({
	Name = "Infinite Coins",
	Min = 10000,
	Max = 50000000,
	Default =10000,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Level",
	Callback = function(Coins)
		game.Players.LocalPlayer.statz.cash.Value = Coins
	end    
})
Tab:AddSlider({
	Name = "Infinite Spins",
	Min = 100000,
	Max = 999999999,
	Default =100000,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Level",
	Callback = function(Spins)
		game.Players.LocalPlayer.statz.spins.Value = Spins
	end    
})

